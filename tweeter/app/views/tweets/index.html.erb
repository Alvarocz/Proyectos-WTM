<h1>Lista de tweets</h1>

<ol>
	<% @tweets.each do |tweet| %>
		<li>
			<%= tweet.message %>
			<%= link_to "Ver", tweet_path(tweet), method: :get %>
			<%= link_to "Editar", edit_tweet_path(tweet) %>
			<%= link_to "Borrar", tweet_path(tweet), method: :delete, data: {confirm: "Desea eliminar este tweet?"} %>
		</li>
	<% end %>
</ol>

<a href="/"><h2>Volver a home</h2></a>