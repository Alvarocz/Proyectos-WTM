<h1>Registrar nuevo estudiante</h1>

<%= form_for @student do |f| %>
  <p>
    <%= f.label "Nombre" %>
  </p>
  <p>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label "Apellido" %>
  </p>
  <p>
    <%= f.text_field :last_name %>
  </p>

  <p>
    <%= f.label "Identificacion" %>
  </p>
  <p>
    <%= f.text_field :identification %>
  </p>

  <p>
    <%= f.label "Email" %>
  </p>
  <p>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.submit "Resgistrar" %>
  </p>
<% end %>

<a href="/"><h3>Ir a home</h3></a>
<a href="/students/"><h3>Atras</h3></a>